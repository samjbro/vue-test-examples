    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "test": "mocha-webpack --webpack-config=node_modules/laravel-mix/setup/webpack.config.js --require resources/assets/js/tests/setup.js resources/assets/js/tests/\\*\\*/\\*.spec.js",
    "test-watch": "mocha-webpack --watch --webpack-config=node_modules/laravel-mix/setup/webpack.config.js  --require resources/assets/js/tests/setup.js resources/assets/js/tests/\\*\\*/\\*.spec.js"
  },
